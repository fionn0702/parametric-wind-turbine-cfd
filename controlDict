FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1000.0;

deltaT          0.01;

writeControl    timeStep;

writeInterval   100;

purgeWrite      1000;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable false;

/*
maxCo          100;       // Your desired Courant number upper limit

adjustTimeStep yes;       // Automatically adjust time step
